{
  "name": "network-scanner-js",
  "version": "1.1.5",
  "description": "A network diagnostics library that can run ping tests, IP scanning, realtime monitoring and subnet calculating along with other network troubleshooting tests",
  "main": "index.js",
  "scripts": {
    "test": "jest --forceExit",
    "build": "babel src -d dist",
    "start": "node -r esm src/index.js"
  },
  "keywords": [
    "promise",
    "async",
    "ping",
    "ssl",
    "mac lookup",
    "subnet calculator",
    "latency test",
    "network programming",
    "web sockets",
    "TCP connections",
    "Traceroute",
    "ReactJS",
    "React",
    "fetch",
    "dns lookup",
    "ssh"
  ],
  "author": "Hugo Page",
  "license": "ISC",
  "readme": "README.md",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/preset-env": "^7.13.9"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "extract-domain": "^2.2.1",
    "fast-speedtest-api": "^0.3.2",
    "get-ssl-certificate": "^2.3.3",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.1",
    "nodejs-traceroute": "^1.2.0",
    "open": "^8.0.2",
    "ping": "^0.2.3",
    "regenerator-runtime": "^0.13.7",
    "socket.io": "^3.0.4",
